      <media:content url="http://video-pdu.us.tomonews.net/mcp/us/648/12/03/14/3403585/3403585_224C8F24AFFA413C8D08EEF8C876A904_170727_3403585____Unique____neutron_star_system___s_activity_ca_2600.mp4" type="video/mp4"/>
